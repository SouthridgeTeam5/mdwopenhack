							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "CuratedAddress2",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"CuratedAddress1": {},
									"CuratedAddress2": {},
									"CloudSalesAddressSink1": {},
									"CloudStreamingAddressSink2": {}
								}
							},
