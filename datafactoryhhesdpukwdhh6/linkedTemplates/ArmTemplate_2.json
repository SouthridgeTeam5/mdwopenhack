									"CloudStreaming": {},
									"FourthCoffeeRentals": {},
									"OnPremRentals": {},
									"CustomerCloudSalesSink": {},
									"CustomerCloudStreamingParquetSink": {},
									"CustomerFourthCoffeeRentalsSink": {},
									"CustomerOnPremRentalsSink": {}
								}
