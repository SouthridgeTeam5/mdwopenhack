{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryhhesdpukwdhh6"
		},
		"ArmtemplateStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ArmtemplateStorageLinkedService'"
		},
		"AzureDataLakeSouthridgeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeSouthridgeStorage'"
		},
		"AzureSqlCloudStreamingDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlCloudStreamingDatabase'"
		},
		"AzureSqlDatabaseSales_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseSales'"
		},
		"CosmosDbMovies_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMovies'"
		},
		"SqlServerVanarsdel_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerVanarsdel'"
		},
		"AzureDataLakeSouthridgeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://southridgedvdteam5.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryhhesdpukwdhh6_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ArmtemplateStorageLinkedService_connectionString": {
						"value": "[parameters('ArmtemplateStorageLinkedService_connectionString')]"
					},
					"AzureDataLakeSouthridgeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeSouthridgeStorage_accountKey')]"
					},
					"AzureSqlCloudStreamingDatabase_connectionString": {
						"value": "[parameters('AzureSqlCloudStreamingDatabase_connectionString')]"
					},
					"AzureSqlDatabaseSales_connectionString": {
						"value": "[parameters('AzureSqlDatabaseSales_connectionString')]"
					},
					"CosmosDbMovies_connectionString": {
						"value": "[parameters('CosmosDbMovies_connectionString')]"
					},
					"AzureDataLakeSouthridgeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeSouthridgeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactoryhhesdpukwdhh6_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServerVanarsdel_connectionString": {
						"value": "[parameters('SqlServerVanarsdel_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactoryhhesdpukwdhh6_ArmTemplate_0')]"
			]
		}
	]
}