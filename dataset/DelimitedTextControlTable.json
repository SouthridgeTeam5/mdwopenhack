{
	"name": "DelimitedTextControlTable",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageSynapse",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Control_Table_Name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().Control_Table_Name,'.csv')\n",
					"type": "Expression"
				},
				"folderPath": "ControlTables",
				"fileSystem": "app"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}